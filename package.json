{
  "name": "@jannchie/eslint-config",
  "type": "module",
  "version": "3.7.3",
  "packageManager": "pnpm@8.8.0",
  "description": "Jannchie's ESLint config",
  "author": "Jianqi Pan <jannchie@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Jannchie",
  "homepage": "https://github.com/Jannchie/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean --dts",
    "stub": "tsup",
    "dev": "tsup --watch & eslint-flat-config-viewer",
    "ui": "npx @eslint/config-inspector --config eslint.config.js",
    "lint": "pnpm run stub && eslint .",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^2.0.1",
    "@next/eslint-plugin-next": ">=15.0.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@unocss/eslint-plugin": ">=0.50.0",
    "astro-eslint-parser": "^1.0.2",
    "eslint": "^9.10.0",
    "eslint-plugin-astro": "^1.2.0",
    "eslint-plugin-format": ">=0.1.0",
    "eslint-plugin-jsx-a11y": ">=6.10.2",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-solid": "^0.14.3",
    "eslint-plugin-svelte": ">=2.35.1",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-slidev": "^1.0.5",
    "svelte-eslint-parser": ">=0.37.0"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "@prettier/plugin-xml": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-vuejs-accessibility": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    },
    "prettier-plugin-slidev": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-config": "^6.4.2"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "vitest": "^4.0.15"
  }
}